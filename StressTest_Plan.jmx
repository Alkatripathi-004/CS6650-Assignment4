            <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import java.time.Instant

  vars.put(&quot;messageId&quot;, UUID.randomUUID().toString())
  vars.put(&quot;userId&quot;, String.valueOf(new Random().nextInt(100000) + 1))
  vars.put(&quot;username&quot;, &quot;user&quot; + vars.get(&quot;userId&quot;))
  vars.put(&quot;timestamp&quot;, Instant.now().toString())

  def messages = [&quot;Hello everyone!&quot;, &quot;How are you?&quot;, &quot;Great discussion!&quot;, &quot;Thanks!&quot;,
   &quot;Good morning!&quot;, &quot;Stress test!&quot;, &quot;High load message&quot;]
  def random = new Random()
  vars.put(&quot;chatMessage&quot;, messages[random.nextInt(messages.size())])

  def rand = random.nextInt(100)
  if (rand &lt; 90) {
      vars.put(&quot;messageType&quot;, &quot;TEXT&quot;)
  } else if (rand &lt; 95) {
      vars.put(&quot;messageType&quot;, &quot;JOIN&quot;)
  } else {
      vars.put(&quot;messageType&quot;, &quot;LEAVE&quot;)
  }
</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
            </JSR223PreProcessor>
